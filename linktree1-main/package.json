{
  "name": "nextjs_ic_template",
  "version": "0.5.0",
  "author": "Henry Chan",
  "description": "Next.js Internet Computer Starter Template",
  "keywords": [
    "nextjs",
    "internet computer",
    "icp",
    "starter",
    "dfinity"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "export": "next build",
    "declaration:generate": "dfx generate",
    "sync:hello": "DFX_NETWORK=local && rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/hello --exclude='idl/' --exclude='*.wasm' --delete ui/declarations",
    "sync:image": "DFX_NETWORK=local && rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/image --exclude='idl/' --exclude='*.wasm' --delete ui/declarations"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.5.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
    "@dfinity/agent": "^0.19.2",
    "@dfinity/candid": "^0.19.2",
    "@dfinity/principal": "^0.19.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@junobuild/core": "^0.0.36",
    "@junobuild/core-peer": "^0.0.1",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "compressorjs": "^1.2.1",
    "events": "^3.3.0",
    "framer-motion": "^10.16.4",
    "next": "^13.5.2",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-dropzone": "^14.2",
    "react-social-icons": "^6.6.0",
    "react-toastify": "^9.1.3",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "uuid": "^9.0.1"
  }
}
